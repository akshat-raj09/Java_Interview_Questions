    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch MainExecutionClass",
            "request": "launch",
            "mainClass": "com.qa.MainClass.MainExecutionClass",
            "vmArgs": "-Dsun.stdout.encoding=UTF-8 -Dsun.err.encoding=UTF-8",
            "projectName": "JavaInterview"
        }
    ]
}